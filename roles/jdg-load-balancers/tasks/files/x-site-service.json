{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "jdg-app-x-site",
    "annotations": {
      "description": "x-site replication endpoint"
    }
  },
  "spec": {
    "ports": [
      {
        "protocol": "TCP",
        "port": 55200,
        "targetPort": 55200
      }
    ],
    "selector": {
      "deploymentConfig": "jdg-app"
    },
    "type": "LoadBalancer"
  }
}
